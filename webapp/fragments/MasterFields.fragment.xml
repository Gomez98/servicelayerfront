<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Table id="idMasterFields" items="{camposModel>/}">
        <headerToolbar>
            <OverflowToolbar>
                <Title text="Campos Maestros" />
            </OverflowToolbar>
        </headerToolbar>
        <columns>
            <Column>
                <Text text="Nombre" />
            </Column>
            <Column>
                <Text text="Creado" />
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{camposModel>name}" />
                </cells>
                  <cells>
                    <Text text="{camposModel>updatedAt}" />
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</core:FragmentDefinition>
